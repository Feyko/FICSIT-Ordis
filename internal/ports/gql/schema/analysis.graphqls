type AnalysisResult {
    smlVersion: String
    gameVersion: String
    commandLine: String
    path: String
    cl: String
    modList: [String!]

    piracyInfo: PiracyInformation
    desiredSMLVersion: String
    crashMatches: [CrashMatch!]
}

type PiracyInformation {
    isPirated: Boolean!
    reason: String!
}

extend type Query {
    analyseFileURL(fileURL: String!): AnalysisResult!
    analyseText(text: String!): AnalysisResult!
}